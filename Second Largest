#include<iostream>
using namespace std;

int main()
{
    int t;
    int s_largest;
    cin>>t;
    for(int i=0;i<t;i++)
    {
        int a,b,c;
        cin>>a>>b>>c;
        if(a>b && b>c)
        s_largest = b;
        else if(b>a && a>c)
        s_largest = a;
        else if(c>a && a>b)
        s_largest = a;
        else if(a>c && c>b)
        s_largest = c;
        else if(b>c && c>a)
        s_largest =c;
        else if (c>b && b>a)
        s_largest =b;
        cout<<s_largest<<endl;
    }
    return 0;
}
