#include<iostream>
using namespace std;


int main()
{

    int T;
    cin >>T;
    for(int i=0;i<T;i++)
    {
        int n,ld=0,ans=0;
        cin>>n;
        ans+=n%10;
        while(n>0)
        {
            ld=n%10;
            n=n/10;
        }
        ans+=ld;
        cout<<ans<<endl;
    }
    return 0;
}
