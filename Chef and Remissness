// Chef and Reminissness
#include<iostream>
using namespace std;

int main()
{
    int t;
    cin >>t;
    for(int i=0;i < t;i++)
    {
        int a,b,sum=0;
        cin >>a>>b;
        int max;
        sum = a+b;
        if(a>b)
        {
            max = a;
        }
        else
        {
            max = b;
        }
        cout<< max <<" "<< sum <<endl;
    }
    return 0;
}
