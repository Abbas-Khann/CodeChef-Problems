import java.util.Scanner;

public class Sum {
    public static void main(String[] args) {

        Scanner input = new Scanner(System.in);
        int t = input.nextInt();
        for(int i = 0; i < t; i++) {
        int a = input.nextInt();
        int b = input.nextInt();
            int sum = a + b;
            System.out.println(sum);
        }
    }
}
